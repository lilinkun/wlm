<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:lzy="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <com.wlm.wlm.ui.CustomTitleBar
        android:id="@+id/titlebar"
        lzy:img_left="@mipmap/ic_back_white"
        lzy:text_color="@color/white"
        lzy:text_title="@string/coupon"
        lzy:title_color="@color/setting_title_color"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></com.wlm.wlm.ui.CustomTitleBar>

    <android.support.v7.widget.RecyclerView
        android:id="@+id/rv_record"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </android.support.v7.widget.RecyclerView>


</LinearLayout>
