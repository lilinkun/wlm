<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="titlebar">
        <attr name="img_left" format="reference" />
        <attr name="img_right" format="reference" />
        <attr name="text_title" format="string" />
        <attr name="title_color" format="color" />
        <attr name="right_color" format="color"/>
        <attr name="text_left" format="string" />
        <attr name="left_TextSize" format="float"/>
        <attr name="right_TextSize" format="dimension"/>
        <attr name="text_right" format="string" />
        <attr name="text_color" format="color" />
        <attr name="left_visibility" format="boolean"/>
        <attr name="text_size" format="float"/>
    </declare-styleable>

    <declare-styleable name="register_layout">

        <attr name="is_register" format="boolean"/>

    </declare-styleable>



    <declare-styleable name="round">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_inside_color" format="color" />
        <attr name="border_outside_color" format="color"></attr>
    </declare-styleable>

    <declare-styleable name="Custom_Round_Image_View">
        <attr name="radius" format="dimension"/>
        <attr name="left_top_radius" format="dimension"/>
        <attr name="right_top_radius" format="dimension"/>
        <attr name="right_bottom_radius" format="dimension"/>
        <attr name="left_bottom_radius" format="dimension"/>
        <attr name="borderColor" format="color"/>
    </declare-styleable>


    <declare-styleable name="BannerView">
        <attr name="dotIndicatorFocus" format="color|reference"/>
        <attr name="dotIndicatorNormal" format="color|reference"/>
        <attr name="dotSize" format="dimension"/>
        <attr name="dotDistance" format="dimension"/>
        <attr name="dotGravity" format="enum">

            <enum name="center" value="0"/>
            <enum name="right" value="1"/>
            <enum name="left" value="-1"/>
        </attr>
        <attr name="bottomColor" format="color|reference"/>

    </declare-styleable>

    <declare-styleable name="DotIndicatorView">

        <attr name="shape" format="enum">
            <enum name="rect" value="1"/>
            <enum name="circle" value="2"/>
        </attr>

    </declare-styleable>


    <declare-styleable name="FlowLayout">
        <attr name="lineSpacing" format="dimension" />
        <attr name="maxLine" format="integer" />
    </declare-styleable>


    <declare-styleable name="TagFlowLayout">
        <attr name="max_select" format="integer"></attr>
        <attr name="tag_gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>


    <declare-styleable name="SlideDetailsLayout">
        <!-- float value for indicate the moment of switch panel-->
        <attr name="percent" format="float"/>
        <!-- how long the animation keep-->
        <attr name="duration" format="integer"/>
        <!-- default panel to show after init-->
        <attr name="default_panel" format="enum">
            <enum name="front" value="0"/>
            <enum name="behind" value="1"/>
        </attr>
    </declare-styleable>


    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="pstsUnderlineColor" format="color" />
        <attr name="pstsTabTextClickColor" format="color" />
        <attr name="pstsTabTextUnclickColor" format="color" />

    </declare-styleable>

    <declare-styleable name="TopLayout">
        <attr name="up_arrow" format="reference"/>
        <attr name="down_arrow" format="reference"/>
        <attr name="textcolor" format="color"/>
        <attr name="textnormalcolor" format="color"/>
        <attr name="type" format="integer"/>

    </declare-styleable>

    <declare-styleable name="CountdownView1">
        <attr format="boolean" name="isHideTimeBackground"/>
        <attr format="boolean" name="isShowTimeBgDivisionLine"/>
        <attr format="color" name="timeBgDivisionLineColor"/>
        <attr format="dimension" name="timeBgDivisionLineSize"/>
        <attr format="color" name="timeBgColor"/>
        <attr format="dimension" name="timeBgSize"/>
        <attr format="dimension" name="timeBgRadius"/>
        <attr format="boolean" name="isTimeTextBold"/>
        <attr format="dimension" name="timeTextSize"/>
        <attr format="color" name="timeTextColor"/>
        <attr format="boolean" name="isShowDay"/>
        <attr format="boolean" name="isShowHour"/>
        <attr format="boolean" name="isShowMinute"/>
        <attr format="boolean" name="isShowSecond"/>
        <attr format="boolean" name="isShowMillisecond"/>
        <attr format="boolean" name="isSuffixTextBold"/>
        <attr format="dimension" name="suffixTextSize"/>
        <attr format="color" name="suffixTextColor"/>
        <attr format="string" name="suffix"/>
        <attr format="string" name="suffixDay"/>
        <attr format="string" name="suffixHour"/>
        <attr format="string" name="suffixMinute"/>
        <attr format="string" name="suffixSecond"/>
        <attr format="string" name="suffixMillisecond"/>
        <attr name="suffixGravity">
            <enum name="top" value="0"/>
            <enum name="center" value="1"/>
            <enum name="bottom" value="2"/>
        </attr>
        <attr format="dimension" name="suffixLRMargin"/>
        <attr format="dimension" name="suffixDayLeftMargin"/>
        <attr format="dimension" name="suffixDayRightMargin"/>
        <attr format="dimension" name="suffixHourLeftMargin"/>
        <attr format="dimension" name="suffixHourRightMargin"/>
        <attr format="dimension" name="suffixMinuteLeftMargin"/>
        <attr format="dimension" name="suffixMinuteRightMargin"/>
        <attr format="dimension" name="suffixSecondLeftMargin"/>
        <attr format="dimension" name="suffixSecondRightMargin"/>
        <attr format="dimension" name="suffixMillisecondLeftMargin"/>
    </declare-styleable>

    <declare-styleable name="UpdataAPPProgressBar">
        <attr name="updataAPPReachedBarColor" format="color" />
        <attr name="updataAPPUnreachedBarColor" format="color" />
        <attr name="updataAPPTextColor" format="color" />
        <attr name="updataAPPBarHeight" format="dimension"/>
        <attr name="updataAPPMax" format="integer" />
        <attr name="updataAPPProgress" format="integer" />
        <attr name="updataAPPSuffix" format="string" />
        <attr name="updataAPPPrefix" format="string" />
        <attr name="updataAPPTextVisibility" format="boolean"/>
    </declare-styleable>

</resources>
